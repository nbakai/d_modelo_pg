<p id="notice"><%= notice %></p>

<h1>Posts</h1>
<%= render 'form', post: @post%>
<div id="my_posts">
  <table>
      <thead>
          <th>Título</th>
          <th>Descripción</th>
          <th>Borrar</th>
      </thead>
      <tbody>
        <% @posts.each do  |post|%>
            <%= render post %>
        <%end%>
     
      </tbody>
  </table>
</div>
<br>


